package app;

public class VetorLinear {
	
	int bola;
	int pontos;
	boolean valor;	
	
	public int getBola() {
		return bola;
	}
	public void setBola(int bola) {
		this.bola = bola;
	}
	public int getPontos() {
		return pontos;
	}
	public void setPontos(int pontos) {
		this.pontos = pontos;
	}
	public boolean isValor() {
		return valor;
	}
	public void setValor(boolean valor) {
		this.valor = valor;
	}
}
