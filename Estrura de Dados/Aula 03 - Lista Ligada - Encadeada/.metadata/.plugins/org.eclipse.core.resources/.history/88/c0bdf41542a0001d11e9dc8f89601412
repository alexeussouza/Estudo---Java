package app;

public class Principal {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		ListaLigada lista = new ListaLigada();
		
		lista.adicionar("AC");
		lista.adicionar("BA");
		lista.adicionar("CE");
		lista.adicionar("DF");
		lista.adicionar("GO");
		lista.adicionar("MA");
		
		System.out.println("Tamanho " + lista.getTamanho());
		System.out.println("Primeiro: " + lista.getPrimeiro().getValor());
		System.out.println("Ultimo: " + lista.getUltimo().getValor());
		
		System.out.println(lista.obterPosicao(0).getValor());
		System.out.println(lista.obterPosicao(1).getValor());
		System.out.println(lista.obterPosicao(2).getValor());
		System.out.println(lista.obterPosicao(3).getValor());
		System.out.println(lista.obterPosicao(4).getValor());
		System.out.println(lista.obterPosicao(5).getValor());
		
		
		lista.removerValor("BA");
		lista.removerValor("CE");
		lista.removerValor("DF");
		lista.removerValor("GO");
		lista.removerValor("MA");
		lista.removerValor("AC");
		System.out.println("Tamanho " + lista.getTamanho());
		
	}
}
