		ListaLigada lista = new ListaLigada();
		
		lista.adicionar("AC");
		lista.adicionar("BA");
		lista.adicionar("CE");
		lista.adicionar("DF");
		lista.adicionar("GO");
		lista.adicionar("MA");
		
		System.out.println("Tamanho " + lista.getTamanho());
		System.out.println("Primeiro" + lista.getPrimeiro().getValor());
		System.out.println("Primeiro" + lista.getUltimo().getValor());

	}
}
