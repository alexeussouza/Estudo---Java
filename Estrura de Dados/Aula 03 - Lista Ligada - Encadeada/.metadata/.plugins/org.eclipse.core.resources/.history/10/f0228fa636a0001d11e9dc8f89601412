package app;

public class Principal {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		ListaLigada lista = new ListaLigada();
		
		lista.adicionar("AC");
		lista.adicionar("BA");
		lista.adicionar("CE");
		lista.adicionar("DF");
		lista.adicionar("GO");
		lista.adicionar("MA");
		
		System.out.println("Tamanho " + lista.getTamanho());
		System.out.println("Primeiro: " + lista.getPrimeiro().getValor());
		System.out.println("Ultimo: " + lista.getUltimo().getValor());
		
		System.out.println(lista.obterPosicao(0));
		System.out.println(lista.obterPosicao(1));
		System.out.println(lista.obterPosicao(2));
		System.out.println(lista.obterPosicao(3));
		System.out.println(lista.obterPosicao(4));
		System.out.println(lista.obterPosicao(5));
		
	}
}
